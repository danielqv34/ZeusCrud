<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>Zeus</display-name>
    <servlet>
        <description>
        </description>
        <display-name>ServletIndex</display-name>
        <servlet-name>ServletIndex</servlet-name>
        <servlet-class>com.dqv.servlets.ServletIndex</servlet-class>
    </servlet>
    <servlet>
        <description>
        </description>
        <display-name>ServletInsert</display-name>
        <servlet-name>ServletInsert</servlet-name>
        <servlet-class>com.dqv.servlets.ServletInsert</servlet-class>
    </servlet>
    <servlet>
        <description>
        </description>
        <display-name>ServletRead</display-name>
        <servlet-name>ServletRead</servlet-name>
        <servlet-class>com.dqv.servlets.ServletRead</servlet-class>
    </servlet>
    <servlet>
        <display-name>servletUpdate</display-name>
        <servlet-name>ServletUpdate</servlet-name>
        <servlet-class>com.dqv.servlets.ServletUpdate</servlet-class>
    </servlet>
    <servlet>
        <description>
        </description>
        <display-name>servletDelete</display-name>
        <servlet-name>ServletDelete</servlet-name>
        <servlet-class>com.dqv.servlets.ServletDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletIndex</servlet-name>
        <url-pattern>/ServletIndex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInsert</servlet-name>
        <url-pattern>/ServletInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletRead</servlet-name>
        <url-pattern>/ServletRead</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletUpdate</servlet-name>
        <url-pattern>/ServletUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDelete</servlet-name>
        <url-pattern>/ServletDelete</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
